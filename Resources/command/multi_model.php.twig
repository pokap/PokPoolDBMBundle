<?php

namespace {{ base_namespace }}\{{ model_namespace }};

class {{ model_name }}
{
{% for name, method in managers %}
    private ${{ name }};
{% endfor %}

{% for name, method in managers %}
    public function {{ method.name }}({{ method.arguments|join(', ') }})
    {
        {% spaceless %}
            {% if 'getter' == method.type %}
                return $this->{{ name }}->{{ method.name }}({{ method.arguments|join(', ') }});
            {% else %}
                $this->{{ name }}->{{ method.name }}();

                return $this;
            {% endif %}
        {% endspaceless %}
    }
{% endfor %}
}
