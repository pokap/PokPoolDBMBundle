{
    "name": "pokap/pool-dbm-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Multi Doctrine Repositories Bundle",
    "keywords": ["multi", "repositories", "symfony", "doctrine", "dbms"],
    "license": "MIT",
    "authors": [
        {"name": "Florent Denis", "email": "dflorent.pokap@gmail.com"}
    ],
    "require": {
        "php": ">=5.3.2",
        "pokap/pool-dbm": "*",
        "symfony/framework-bundle": "2.*",
        "symfony/doctrine-bridge": "2.*"
    },
    "require-dev": {
        "symfony/finder": "*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Pok\\Bundle\\PoolDBMBundle": ""
        }
    },
    "target-dir": "Pok/Bundle/PoolDBMBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
